<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/primary_black">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/default_corner_radius"/>
            <stroke
                android:color="@color/primary_black"
                android:dashWidth="10dp"
                android:dashGap="5dp"
                android:width="@dimen/default_stroke_width"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/primary_black" />
            <corners android:radius="@dimen/default_corner_radius" />
        </shape>
    </item>
</ripple>